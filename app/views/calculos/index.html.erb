<div class="page-header">
  <h1>Calculos realizados</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Perdida de presion</th>
      <th>Diametro del cilindro</th>
      <th>Velocidad del cilindro</th>
      <th>Presion de trabajo</th>
      <th>Diametro del tubo</th>
      <th>Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @calculos.each do |calculo| %>
      <tr>
        <td><%= calculo.perdida_persion %></td>
        <td><%= calculo.diametro_cilindro %></td>
        <td><%= calculo.velocidad_cilindro %></td>
        <td><%= calculo.presion_trabajo %></td>
        <td><%= calculo.diametro_tubo %></td>
        <td><%= link_to 'Ver', calculo, :class => 'btn btn-small' %>
            <%= link_to 'Editar', edit_calculo_path(calculo), :class => 'btn btn-small' %>
            <%= link_to 'Eliminar', calculo, method: :delete, data: { confirm: 'Â¿Estas seguro?' }, :class => 'btn btn-small btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo calculo', new_calculo_path, :class => 'btn btn-primary' %>
