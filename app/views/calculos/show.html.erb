<div class="page-header">
  <h1>Calculo</h1>
</div>

<p id="notice"><%= notice %></p>

<dl class="dl-horizontal">
  <dt><strong>Perdida de presion:</strong></dt>
  <dd><%= @calculo.perdida_persion %></dd>

  <dt><strong>Diametro del cilindro:</strong></dt>
  <dd><%= @calculo.diametro_cilindro %></dd>

  <dt><strong>Velocidad del cilindro:</strong></dt>
  <dd><%= @calculo.velocidad_cilindro %></dd>

  <dt><strong>Presion de trabajo:</strong></dt>
  <dd><%= @calculo.presion_trabajo %></dd>

  <dt><strong>Diametro del tubo:</strong></dt>
  <dd><%= @calculo.diametro_tubo %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.atras")),
              calculos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.editar")),
              edit_calculo_path(@calculo), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.eliminar")),
              calculo_path(@calculo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Â¿Estas seguro?')) },
              :class => 'btn btn-danger' %>
</div>
